export function sendForm(method, form, destination) {
  const xhr = new XMLHttpRequest();
  xhr.open(method, destination);
  xhr.send(new FormData(form));
  xhr.onload = () => {
    if (xhr.status === 200) {
      return false;
    }
    return false;
  };
}

export function returnFalse() {
  return false;
}
